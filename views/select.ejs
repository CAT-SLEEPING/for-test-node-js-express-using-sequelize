<h3>Book Data</h3>

<table border="1">
    <thead>
        <tr>
            <th>id</th>
            <th>barcode</th>
            <th>isbn</th>
            <th>name</th>   
            <th>price</th>
            <th>cost</th>
            <th width="110px"></th>
         </tr>
    </thead>
    <tbody>
        <% books.forEach(item=>{%>
        <tr>
            <td><%= item.id %></td>
            <td><%= item.barcode %></td>
            <td><%= item.isbn %></td>
            <td><%= item.name %></td>
            <td><%= item.price %></td>
            <td><%= item.cost %></td>
            <td style="text-align:center">
                <a href="/edit/<%=item.id%">Edit</a>
                <a href="/delete/<%=item.id%">Edit</a>
            </td>>
        </tr>
        <%});%>
    </tbody>
</table>