<H1>CAT SHOP</H1>
<form method="post">
    <div>
        <label>Barcode</label>
        <input name="Barcode" value="<%= data.barcode %>"/>
    </div>
    <div>
        <label>Name</label>
        <input name="name" value="<%= data.name %>"/>
    </div>
    <div>
        <label>ISBN</label>
        <input name="isbn" value="<%= data.isbn %>"/>
    </div>
    <div>
        <label>Price</label>
        <input name="price" value="<%= data.price %>"/>
    </div>
    <div>
        <label>Cost</label>
        <input name="cost" value="<%= data.cost %>"/>
    </div>
    <button type="submit">Save</button>
</form>